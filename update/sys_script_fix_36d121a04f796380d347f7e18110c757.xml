<record_update table="sys_script_fix">
    <sys_script_fix action="INSERT_OR_UPDATE">
        <active>true</active>
        <before>false</before>
        <description/>
        <flush_cache>false</flush_cache>
        <name>Create Categories</name>
        <run_once>false</run_once>
        <script><![CDATA[(function () {
	var gr = new GlideRecord('x_wegu_te_category');
	gr.query();
	
	while (gr.next()) {
		var data = {};
		data.name = gr.getValue('name');
		data.active = gr.getDisplayValue('active');
		
		createCategory(data);
	}
	
	function createCategory (data) {
		var gr = new GlideRecord('x_wegu_er_category');
		gr.newRecord();
		gr.setValue('name', data.name);
		gr.setValue('active', data.active);
		gr.insert();
	}
	
})();]]></script>
