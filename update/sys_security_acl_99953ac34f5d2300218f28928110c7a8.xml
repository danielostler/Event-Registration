<record_update table="sys_security_acl">
    <sys_security_acl action="INSERT_OR_UPDATE">
        <active>true</active>
        <admin_overrides>true</admin_overrides>
        <advanced>true</advanced>
        <condition/>
        <description>Allow read for registered_users in x_wegu_er_instance, if the ACL script returns true.</description>
        <name>x_wegu_er_instance.registered_users</name>
        <operation display_value="read">read</operation>
        <script><![CDATA[var arrayUtil = new global.ArrayUtil();
var options = current.getValue('options').split(',');
var users = current.getValue('registered_users').split(',');

if (arrayUtil.contains(options, 'Public Participants List') || arrayUtil.contains(users, gs.getUserID()) || gs.hasRole('x_wegu_er.user')) {
	answer = true;
}]]></script>
